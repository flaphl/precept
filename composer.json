{
    "name": "flaphl/precept",
    "description": "Contract-driven ORM for Flaphl framework with graceful evolution",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jade Phyressi",
            "email": "jade@flaphl.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/event-dispatcher": "^1.0",
        "psr/cache": "^3.0",
        "psr/container": "^2.0",
        "flaphl/cache-contracts": "^1.0",
        "flaphl/injection": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Flaphl\\Fridge\\Precept\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
